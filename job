# /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_update/MAIN_CSH/cactus_opt_12z.sh
# /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_ecmwf/MAIN_CSH/cactus_ecm_00z.sh
