# Copy Yan Luo directories to 4 sub-directories: run_web, ccpa, cqpf and harch

home=/lfs/h2/emc/vpppg/save/bo.cui 

mkdir $home/run_web 
cd $home/run_web

cp -p /lfs/h2/emc/vpppg/save/yan.luo/tskr_prec/3hr_CCPA/run/CCPA_archive_cactus .
cp -p /u/yan.luo/save/arch/test_cactus .
cp -p /u/yan.luo/save/arch/GARCH_cactus .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work/MAIN_CSH/cactus_def_00z.sh .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work/MAIN_CSH/cactus_def_12z.sh .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_update/MAIN_CSH/cactus_opt_00z.sh .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_update/MAIN_CSH/cactus_opt_12z.sh .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_ecmwf/MAIN_CSH/cactus_ecm_00z.sh .
cp -p /lfs/h2/emc/vpppg/save/yan.luo/CSTAR/work_ecmwf/MAIN_CSH/cactus_ecm_12z.sh .

 
# Introduction to each job:
# CCPA_archive_cactus
# Function: archive real-time CCPA data and plot CCPA images and send them to the webpage.
# Webpage: https://www.emc.ncep.noaa.gov/gmb/yluo/ccpa/ccpa.php
# Required files under the following directories: 

mkdir $home/ccpa
cd $home/ccpa
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/tskr_prec  .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/plot_ccpa .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/grads .
cp -rp /u/yan.luo/save/xbin .

# test_cactus
# Function: plot NAEFS/CQPF images and post them to the webpages
# Webpages:
# https://www.emc.ncep.noaa.gov/gmb/yluo/GEFS_VRFY/CPQPF_6h.html
# https://www.emc.ncep.noaa.gov/gmb/yluo/GEFS_VRFY/CPQPF_24h.html
# Required files under the following directories:

mkdir $home/cqpf
cd $home/cqpf
cp -rp /u/yan.luo/save/arch/test_cactus .
cp -rp /u/yan.luo/save/plot_cqpf .
cp -rp /u/yan.luo/save/grads .
cp -rp /u/yan.luo/save/xbin .

# GARCH_cactus
# Function: legacy web jobs, plot ensemble web products and post to the webpages
# Webpages:
# https://www.emc.ncep.noaa.gov/gmb/yluo/html_pqpf/pqpf_us1.html
# https://www.emc.ncep.noaa.gov/gmb/yluo/html_pqpf/rmop.html
# Required files under the following directories:

mkdir $home/garch
cd $home/garch

cp -rp /u/yan.luo/save/arch/GARCH_cactus .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/earch_00 .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/eprob .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/evrfy .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/gvrfy .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/sbin .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/bin .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/web_glb_js .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/wpqpf .
cp -rp /lfs/h2/emc/vpppg/save/yan.luo/grads .
